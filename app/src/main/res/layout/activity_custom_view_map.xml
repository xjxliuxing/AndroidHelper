<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xjx.helper.ui.home.activity.tod.customviewui.CustomViewMapActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_1"
            style="@style/style_todo_item"
            android:text="自定义DrawableTextView" />

        <TextView
            android:id="@+id/tv_2"
            style="@style/style_todo_item"
            android:text="自定义RecycleView布局" />

        <TextView
            android:id="@+id/tv_3"
            style="@style/style_todo_item"
            android:text="自定义横向九宫格" />

        <TextView
            android:id="@+id/tv_4"
            style="@style/style_todo_item"
            android:text="自定义进度条" />

        <TextView
            android:id="@+id/tv_5"
            style="@style/style_todo_item"
            android:text="自定义密码框" />

        <TextView
            android:id="@+id/tv_6"
            style="@style/style_todo_item"
            android:text="自定义闪电进度条" />

        <TextView
            android:id="@+id/tv_7"
            style="@style/style_todo_item"
            android:text="自定义随机分布View的布局" />

        <TextView
            android:id="@+id/tv_8"
            style="@style/style_todo_item"
            android:text="自定义圆角图片" />

    </LinearLayout>
</androidx.core.widget.NestedScrollView>
